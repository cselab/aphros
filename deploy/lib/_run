#!/bin/bash

set -eu

. ch.setenv
. ../util

e () {
  echo "$@"
  eval "$@"
}

l="h5 march overlap vof vofi"
for d in $l ; do
  echo Entering $d
  e "(cd $d && ./conf && make install)"
done

l="csv table vtkcolor"
for d in $l
do (
    e cd $d
    echo Entering `pwd`
    e make install PRE=$PRE
) done
